{
  "options": [
    {
      "type": "switch",
      "name": "Autoplay",
      "key": "autoplay",
      "defaultValue": true
    },
    {
      "type": "switch",
      "name": "Mute",
      "key": "mute",
      "defaultValue": true
    },
    {
      "type": "slider",
      "name": "Volume",
      "key": "volume",
      "defaultValue": 100,
      "isInteger": true,
      "range": {
        "start": 0,
        "end": 100
      },
      "visibleIf": {
        "type": "equals",
        "key": "mute",
        "value": false
      }
    },
    {
      "type": "switch",
      "name": "Allow Input",
      "key": "input",
      "defaultValue": false
    },
    {
      "type": "switch",
      "name": "Show Controls",
      "key": "controls",
      "defaultValue": false,
      "visibleIf": {
        "type": "equals",
        "key": "input",
        "value": true
      }
    },
    {
      "type": "switch",
      "name": "Loop",
      "key": "loop",
      "defaultValue": true
    },
    {
      "type": "slider",
      "name": "Scale",
      "key": "scale",
      "defaultValue": 1.0,
      "range": {
        "start": 0.0,
        "end": 2.0
      }
    }
  ]
}
